{
	"name": "skyra",
	"version": "6.0.0",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"main": "dist/Skyra.js",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#lib/audio": "./dist/audio/lib/index.js",
		"#lib/customCommands": "./dist/lib/customCommands/index.js",
		"#lib/database": "./dist/lib/database/index.js",
		"#lib/database/keys": "./dist/lib/database/keys/index.js",
		"#lib/discord": "./dist/lib/discord/index.js",
		"#lib/moderation": "./dist/lib/moderation/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/structures/color": "./dist/lib/structures/color/index.js",
		"#lib/setup": "./dist/lib/setup/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#lib/i18n/languageKeys": "./dist/lib/i18n/languageKeys/index.js",
		"#root/*": "./dist/*.js",
		"#languages": "./dist/languages/index.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"start": "node dist/Skyra.js",
		"lint": "eslint --fix src tests --ext ts",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"scripts:tlds": "node scripts/tlds.mjs",
		"scripts:migration": "node scripts/migrations.mjs",
		"watch": "tsc -b src -w",
		"dev": "yarn build && yarn start",
		"dockerps": "pwsh -NoLogo ./docker/control.ps1",
		"typeorm:schema:resync": "docker container rm -f pgsql && docker volume rm postgres-data && docker volume create postgres-data && docker-compose -p skyra -f ./docker/docker-compose.yml up -d postgres && sleep 3 && yarn typeorm schema:sync",
		"postinstall": "husky install .github/husky"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/skyra.git"
	},
	"keywords": [
		"discord.js",
		"sapphire",
		"bot",
		"standalone"
	],
	"private": true,
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/skyra-project/skyra/issues"
	},
	"dependencies": {
		"@discordjs/collection": "^0.1.6",
		"@influxdata/influxdb-client": "^1.10.0",
		"@influxdata/influxdb-client-apis": "^1.10.0",
		"@sapphire/async-queue": "^1.0.4",
		"@sapphire/decorators": "^2.0.5",
		"@sapphire/discord.js-utilities": "^1.5.0",
		"@sapphire/framework": "^1.0.0-alpha.11",
		"@sapphire/plugin-api": "^2.0.3",
		"@sapphire/plugin-i18next": "^1.3.2",
		"@sapphire/plugin-logger": "1.0.8",
		"@sapphire/plugin-subcommands": "^1.0.1",
		"@sapphire/snowflake": "^1.2.5",
		"@sapphire/stopwatch": "^1.0.5",
		"@sapphire/time-utilities": "^1.2.10",
		"@sapphire/type": "^1.1.5",
		"@sapphire/utilities": "^1.4.5",
		"@sentry/integrations": "^6.2.0",
		"@sentry/node": "^6.2.0",
		"@skyra/ai": "^1.1.0",
		"@skyra/audio": "^1.0.2",
		"@skyra/char": "^1.0.1",
		"@skyra/editable-commands": "^1.0.0",
		"@skyra/tags": "^1.1.0",
		"async-rwlock": "^1.1.1",
		"binarytf": "^2.0.0",
		"bufferutil": "^4.0.3",
		"canvas": "^2.6.1",
		"canvas-constructor": "^4.1.0",
		"colorette": "^1.2.2",
		"confusables": "^1.0.0",
		"diff": "^5.0.0",
		"discord.js": "^12.5.1",
		"gifencoder": "^2.0.1",
		"he": "^1.2.0",
		"ioredis": "^4.23.0",
		"node-fetch": "^2.6.1",
		"pg": "^8.5.1",
		"tslib": "1.11.2",
		"typeorm": "0.2.28",
		"typeorm-naming-strategies": "^2.0.0",
		"utf-8-validate": "^5.0.4",
		"ws": "^7.4.3",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.0.1",
		"@commitlint/config-conventional": "^12.0.1",
		"@favware/graphql-pokemon": "^5.1.2",
		"@playlyfe/gql": "^2.6.2",
		"@sapphire/eslint-config": "^3.0.1",
		"@sapphire/prettier-config": "^1.0.7",
		"@sapphire/ts-config": "^2.0.1",
		"@skyra/saelem": "^3.0.2",
		"@types/backoff": "^2.5.1",
		"@types/diff": "^5.0.0",
		"@types/he": "^1.1.1",
		"@types/i18next-fs-backend": "^1.0.0",
		"@types/ioredis": "^4.22.0",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.31",
		"@types/node-fetch": "^2.5.8",
		"@types/pg": "^7.14.10",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.12.1",
		"eslint": "^7.21.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^5.1.1",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"jest-mock-random": "^1.1.1",
		"lint-staged": "^10.5.4",
		"nock": "^13.0.9",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"ts-jest": "^26.5.2",
		"ts-node": "^9.1.1",
		"typescript": "4.2.2"
	},
	"resolutions": {
		"acorn": "^8.0.5",
		"apollo-codegen": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"chalk": "^4.1.0",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"kind-of": "^6.0.3",
		"lodash": "^4.17.21",
		"minimist": "^1.2.5",
		"node-ipc": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz"
	},
	"engines": {
		"node": ">=15",
		"npm": ">=6"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config"
}
