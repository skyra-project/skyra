{
	"name": "skyra",
	"version": "6.0.0",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Skyra Project",
	"license": "Apache-2.0",
	"main": "./dist/Skyra.js",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#lib/audio": "./dist/audio/lib/index.js",
		"#lib/birthday": "./dist/lib/birthday/index.js",
		"#lib/customCommands": "./dist/lib/customCommands/index.js",
		"#lib/database": "./dist/lib/database/index.js",
		"#lib/database/keys": "./dist/lib/database/keys/index.js",
		"#lib/database/settings": "./dist/lib/database/settings/index.js",
		"#lib/discord": "./dist/lib/discord/index.js",
		"#lib/env": "./dist/lib/env/index.js",
		"#lib/grpc": "./dist/lib/grpc/index.js",
		"#lib/moderation": "./dist/lib/moderation/index.js",
		"#lib/moderation/workers": "./dist/lib/moderation/workers/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/structures/color": "./dist/lib/structures/color/index.js",
		"#lib/setup": "./dist/lib/setup/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#lib/weather": "./dist/lib/weather/index.js",
		"#lib/i18n/languageKeys": "./dist/lib/i18n/languageKeys/index.js",
		"#root/*": "./dist/*.js",
		"#languages": "./dist/languages/index.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#utils/common": "./dist/lib/util/common/index.js",
		"#utils/functions": "./dist/lib/util/functions/index.js"
	},
	"scripts": {
		"build": "run-s build:*",
		"build:grpc": "node scripts/build/grpc.mjs",
		"build:tsc": "tsc -b src",
		"dev": "yarn build && yarn start",
		"watch": "tsc -b src -w",
		"clean": "node scripts/build/clean.mjs",
		"start": "node dist/Skyra.js",
		"start:profiler:0x": "0x --collect-only dist/Skyra.js",
		"test": "jest",
		"lint": "eslint --fix --ext ts src tests",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"scripts:tlds": "node scripts/tlds.mjs",
		"scripts:migration": "node scripts/migrations.mjs",
		"dockerps": "pwsh -NoLogo ./docker/control.ps1",
		"typeorm:schema:resync": "docker container rm -f postgres && docker volume rm postgres-data && docker volume create postgres-data && docker-compose -f ./docker/docker-compose.yml up -d postgres && sleep 3 && yarn typeorm schema:sync",
		"update": "yarn upgrade-interactive --latest",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/collection": "^0.1.6",
		"@favware/zalgo": "^3.1.2",
		"@grpc/grpc-js": "^1.3.6",
		"@influxdata/influxdb-client": "^1.15.0",
		"@influxdata/influxdb-client-apis": "^1.15.0",
		"@sapphire/async-queue": "^1.1.4",
		"@sapphire/decorators": "^2.2.0",
		"@sapphire/discord.js-utilities": "^2.0.2",
		"@sapphire/fetch": "^1.0.5",
		"@sapphire/framework": "^1.0.2",
		"@sapphire/plugin-api": "^2.2.0",
		"@sapphire/plugin-i18next": "^1.3.7",
		"@sapphire/plugin-logger": "1.0.13",
		"@sapphire/plugin-subcommands": "^1.0.6",
		"@sapphire/ratelimits": "^2.0.1",
		"@sapphire/snowflake": "^2.0.0",
		"@sapphire/stopwatch": "^1.1.4",
		"@sapphire/time-utilities": "^1.3.8",
		"@sapphire/type": "^2.0.11",
		"@sapphire/utilities": "^2.0.1",
		"@sentry/integrations": "^6.11.0",
		"@sentry/node": "^6.11.0",
		"@skyra/ai": "^1.2.0",
		"@skyra/audio": "^1.2.1",
		"@skyra/char": "^1.0.2",
		"@skyra/editable-commands": "^1.0.0",
		"@skyra/gifenc": "^1.0.0",
		"@skyra/saelem": "^3.0.9",
		"@skyra/tags": "^1.1.3",
		"async-rwlock": "^1.1.1",
		"binarytf": "^2.0.0",
		"bufferutil": "^4.0.3",
		"canvas-constructor": "^5.0.1",
		"colorette": "^1.2.2",
		"confusables": "^1.0.0",
		"diff": "^5.0.0",
		"discord-api-types": "^0.18.1",
		"discord.js": "^12.5.3",
		"dotenv-cra": "^3.0.1",
		"grpc_tools_node_protoc_ts": "^5.3.1",
		"he": "^1.2.0",
		"ioredis": "^4.27.7",
		"pg": "^8.6.0",
		"skia-canvas": "^0.9.23",
		"tslib": "1.11.2",
		"typeorm": "0.2.28",
		"typeorm-naming-strategies": "^2.0.0",
		"utf-8-validate": "^5.0.5",
		"ws": "^7.5.3",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"0x": "^4.10.2",
		"@babel/core": "^7.15.0",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-decorators": "^7.14.5",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-typescript": "^7.15.0",
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@favware/graphql-pokemon": "^5.5.9",
		"@sapphire/eslint-config": "^3.2.3",
		"@sapphire/prettier-config": "^1.1.6",
		"@sapphire/ts-config": "^3.0.0",
		"@skyra/star-wars-api": "^2.0.0",
		"@types/backoff": "^2.5.2",
		"@types/diff": "^5.0.1",
		"@types/google-protobuf": "^3.15.3",
		"@types/he": "^1.1.2",
		"@types/i18next-fs-backend": "^1.0.1",
		"@types/ioredis": "^4.26.7",
		"@types/jest": "^26.0.24",
		"@types/node": "^16.4.13",
		"@types/node-fetch": "^2.5.12",
		"@types/pg": "^8.6.1",
		"@types/skia-canvas": "^0.9.1",
		"@types/ws": "^7.4.7",
		"babel-plugin-const-enum": "^1.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"grpc-tools": "^1.11.2",
		"husky": "^7.0.1",
		"jest": "^27.0.6",
		"jest-circus": "^27.0.6",
		"jest-mock-random": "^1.1.1",
		"lint-staged": "^11.1.2",
		"nock": "^13.1.1",
		"npm-run-all": "^4.1.5",
		"pretty-quick": "^3.1.1",
		"ts-node": "^10.1.0",
		"typescript": "4.3.5"
	},
	"resolutions": {
		"acorn": "^8.4.1",
		"chalk": "^4.1.1",
		"diff": "^5.0.0",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
		"lodash": "^4.17.21",
		"minimist": "^1.2.5"
	},
	"engines": {
		"node": "^16",
		"npm": "^7"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/skyra.git"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config"
}
