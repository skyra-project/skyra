{
	"name": "skyra",
	"version": "5.5.0",
	"description": "Multipurpose Discord Bot built on Klasa",
	"main": "dist/Skyra.js",
	"imports": {
		"#utils/*": "./dist/lib/util/*.js",
		"#utils/debounce": "./dist/lib/util/debounce/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/database": "./dist/lib/database/index.js",
		"#lib/database/tableKeys": "./dist/lib/database/tableKeys/index.js",
		"#lib/audio": "./dist/lib/audio/index.js",
		"#lib/misc": "./dist/lib/misc/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#lib/structures/color": "./dist/lib/structures/color/index.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"pm2:delete": "pm2 delete Skyra",
		"pm2:logs": "pm2 logs Skyra --lines=200",
		"pm2:restart": "pm2 restart Skyra",
		"pm2:start": "pm2 start pm2.ecosystem.yml",
		"pm2:dev": "yarn pm2:start --env development",
		"pm2:stop": "pm2 stop Skyra",
		"start": "node dist/Skyra.js",
		"lint": "eslint --fix src tests --ext ts",
		"format": "prettier --write --loglevel=warn \"{src,tests}/**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"postbuild": "node scripts/copy-lua-files.mjs",
		"clean": "tsc -b src --clean",
		"scripts:tlds": "node scripts/tlds.mjs",
		"scripts:migration": "node scripts/migrations.mjs",
		"watch": "tsc -b src -w",
		"dev": "yarn build && yarn start",
		"dockerps": "pwsh -NoLogo ./docker/control.ps1",
		"typeorm:schema:resync": "docker container rm -f pgsql && docker volume rm postgres-data && docker volume create postgres-data && docker-compose -p skyra -f ./docker/docker-compose.yml up -d postgres && sleep 3 && yarn typeorm schema:sync"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/skyra.git"
	},
	"keywords": [
		"discord.js",
		"klasa",
		"bot",
		"standalone"
	],
	"private": true,
	"author": "kyranet <kyradiscord@gmail.com>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/skyra-project/skyra/issues"
	},
	"dependencies": {
		"@discordjs/collection": "^0.1.6",
		"@influxdata/influxdb-client": "^1.8.0",
		"@influxdata/influxdb-client-apis": "^1.8.0",
		"@klasa/async-queue": "^0.0.1",
		"@sapphire/snowflake": "^1.2.0",
		"@sapphire/time-utilities": "^1.2.0",
		"@sapphire/type": "^1.1.0",
		"@sapphire/utilities": "^1.1.1",
		"@sentry/integrations": "^5.27.6",
		"@sentry/node": "^5.27.6",
		"@skyra/ai": "^1.0.2",
		"@skyra/audio": "^1.0.1",
		"@skyra/char": "^1.0.1",
		"@skyra/decorators": "^3.0.0",
		"async-rwlock": "^1.1.1",
		"binarytf": "^2.0.0",
		"bufferutil": "^4.0.2",
		"canvas": "^2.6.1",
		"canvas-constructor": "^4.1.0",
		"colorette": "^1.2.1",
		"confusables": "^1.0.0",
		"diff": "^5.0.0",
		"discord.js": "^12.5.0",
		"emoji-regex": "^9.2.0",
		"gifencoder": "^2.0.1",
		"he": "^1.2.0",
		"ioredis": "^4.19.2",
		"klasa": "skyra-project/klasa#settings",
		"klasa-dashboard-hooks": "skyra-project/klasa-dashboard-hooks#master",
		"node-fetch": "^2.6.1",
		"pg": "^8.5.1",
		"tslib": "1.11.2",
		"tsyringe": "^4.4.0",
		"typeorm": "0.2.28",
		"typeorm-naming-strategies": "^2.0.0",
		"utf-8-validate": "^5.0.3",
		"ws": "^7.4.0",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@favware/graphql-pokemon": "^4.5.0",
		"@playlyfe/gql": "^2.6.2",
		"@sapphire/eslint-config": "^1.0.4",
		"@sapphire/ts-config": "^1.0.3",
		"@skyra/saelem": "^2.0.4",
		"@types/backoff": "^2.5.1",
		"@types/diff": "^4.0.2",
		"@types/he": "^1.1.1",
		"@types/ioredis": "^4.17.8",
		"@types/jest": "^26.0.15",
		"@types/node": "^14.14.9",
		"@types/node-fetch": "^2.5.7",
		"@types/pg": "^7.14.7",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.9.1",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"jest-circus": "^26.6.3",
		"jest-mock-random": "^1.1.1",
		"lint-staged": "^10.5.1",
		"nock": "^13.0.5",
		"prettier": "^2.2.0",
		"pretty-quick": "^3.1.0",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.0.0",
		"typescript": "^4.1.2"
	},
	"resolutions": {
		"@bcoe/v8-coverage": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"acorn": "^8.0.4",
		"apollo-codegen": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"chalk": "^4.1.0",
		"fs-nextra": "^0.5.1",
		"jest-environment-jsdom": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"jest-jasmine2": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
		"kind-of": "^6.0.3",
		"lodash": "^4.17.20",
		"minimist": "^1.2.5",
		"node-ipc": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=6"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
