{
	"name": "skyra",
	"version": "6.3.0",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Skyra Project",
	"license": "Apache-2.0",
	"main": "./dist/Skyra.js",
	"type": "module",
	"imports": {
		"#lib/database": "./dist/lib/database/index.js",
		"#lib/database/entities": "./dist/lib/database/entities/index.js",
		"#lib/database/keys": "./dist/lib/database/keys/index.js",
		"#lib/database/settings": "./dist/lib/database/settings/index.js",
		"#lib/discord": "./dist/lib/discord/index.js",
		"#lib/moderation": "./dist/lib/moderation/index.js",
		"#lib/moderation/managers": "./dist/lib/moderation/managers/index.js",
		"#lib/moderation/workers": "./dist/lib/moderation/workers/index.js",
		"#lib/structures": "./dist/lib/structures/index.js",
		"#lib/structures/managers": "./dist/lib/structures/managers/index.js",
		"#lib/setup": "./dist/lib/setup/index.js",
		"#lib/types": "./dist/lib/types/index.js",
		"#lib/i18n/languageKeys": "./dist/lib/i18n/languageKeys/index.js",
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#languages": "./dist/languages/index.js",
		"#utils/common": "./dist/lib/util/common/index.js",
		"#utils/functions": "./dist/lib/util/functions/index.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"build": "tsc -b src",
		"dev": "yarn build && yarn start",
		"watch": "tsc -b src -w",
		"watch:start": "tsc-watch -b src --onSuccess \"yarn start\"",
		"clean": "node scripts/build/clean.mjs",
		"start": "node --enable-source-maps dist/Skyra.js",
		"start:profiler:0x": "0x --collect-only dist/Skyra.js",
		"test": "vitest run",
		"lint": "eslint --fix --ext ts src tests",
		"format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\"",
		"scripts:tlds": "node scripts/tlds.mjs",
		"scripts:migration": "node scripts/migrations.mjs",
		"dockerps": "pwsh -NoLogo ./.docker/control.ps1",
		"typeorm:schema:resync": "docker container rm -f postgres && docker volume rm postgres-data && docker volume create postgres-data && docker-compose -f ./.docker/docker-compose.yml up -d postgres && sleep 3 && yarn typeorm schema:sync",
		"update": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@discordjs/builders": "^1.7.0",
		"@discordjs/collection": "^2.0.0",
		"@discordjs/core": "^1.1.1",
		"@influxdata/influxdb-client": "^1.33.2",
		"@influxdata/influxdb-client-apis": "^1.33.2",
		"@sapphire/async-queue": "^1.5.1",
		"@sapphire/bitfield": "^1.2.1",
		"@sapphire/decorators": "^6.0.3",
		"@sapphire/discord.js-utilities": "7.1.5",
		"@sapphire/fetch": "^3.0.1",
		"@sapphire/framework": "5.0.5",
		"@sapphire/plugin-api": "^6.0.1",
		"@sapphire/plugin-editable-commands": "^4.0.1",
		"@sapphire/plugin-i18next": "^7.0.1",
		"@sapphire/plugin-logger": "^4.0.1",
		"@sapphire/plugin-subcommands": "^6.0.2",
		"@sapphire/ratelimits": "^2.4.8",
		"@sapphire/snowflake": "^3.5.2",
		"@sapphire/stopwatch": "^1.5.1",
		"@sapphire/time-utilities": "^1.7.11",
		"@sapphire/utilities": "^3.15.1",
		"@sentry/integrations": "^7.91.0",
		"@sentry/node": "^7.91.0",
		"@skyra/ai": "^1.2.0",
		"@skyra/char": "^1.0.3",
		"@skyra/env-utilities": "^1.3.0",
		"@skyra/jaro-winkler": "^1.1.1",
		"async-rwlock": "^1.1.1",
		"bufferutil": "^4.0.8",
		"colorette": "^2.0.20",
		"confusables": "^1.1.1",
		"diff": "^5.1.0",
		"discord-api-types": "~0.37.67",
		"discord.js": "^14.14.1",
		"he": "^1.2.0",
		"pg": "^8.11.3",
		"typeorm": "0.3.17",
		"typeorm-naming-strategies": "^4.1.0",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"0x": "^5.7.0",
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@sapphire/eslint-config": "^5.0.3",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.0",
		"@types/backoff": "^2.5.5",
		"@types/diff": "^5.0.9",
		"@types/he": "^1.2.3",
		"@types/node": "^20.10.6",
		"@types/pg": "^8.10.9",
		"@types/ws": "^8.5.10",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"@vitest/coverage-v8": "^1.1.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.3.3",
		"vitest": "^1.1.1"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"engines": {
		"node": "^20",
		"npm": "^10.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/skyra.git"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write",
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@4.0.2",
	"volta": {
		"node": "20.10.0"
	}
}
