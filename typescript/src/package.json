{
	"name": "skyra",
	"version": "6.0.0",
	"description": "Multipurpose Discord Bot built on Sapphire Framework",
	"author": "Skyra Project",
	"license": "Apache-2.0",
	"main": "../dist/Skyra.js",
	"scripts": {
		"lint": "eslint --fix . --ext ts",
		"format": "prettier --write --loglevel=warn \"**/*.ts\"",
		"build": "run-s scripts:grpc build:tsc",
		"build:tsc": "tsc -b .",
		"watch": "tsc -b . -w",
		"clean": "node ../../scripts/build/clean.mjs",
		"scripts:grpc": "node ../../scripts/build/grpc.mjs",
		"dev": "yarn build && yarn start"
	},
