		"@sapphire/discord.js-utilities": "^1.5.8",
		"@sapphire/fetch": "^1.0.2",
		"@sapphire/framework": "^1.0.2",
		"@sapphire/plugin-api": "^2.1.4",
		"@sapphire/plugin-i18next": "^1.3.6",
		"@sapphire/plugin-logger": "1.0.12",
		"@sapphire/plugin-subcommands": "^1.0.5",
